{
  "version": 2,
  "builds": [
     {
        "src": "./index.js",
        "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api-docs/swagger.json",
      "dest": "/api-docs/swagger.json"
    },
    {
      "src": "/api-docs",
      "dest": "/api-docs"
    },
    {
      "src": "/swagger.html",
      "dest": "/swagger.html"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "index.js": {
      "maxDuration": 30
    }
  }
}
