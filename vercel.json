{
  "version": 2,
  "builds": [
     {
        "src": "./index.js",
        "use": "@vercel/node"
    }
],
  "routes": [
    {
      "src": "/api-docs/(.*)",
      "dest": "/api-docs/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "index.js": {
      "maxDuration": 30
    }
  }
}
